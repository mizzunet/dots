#!/usr/bin/bash
# Start pipewire,wireplumber and pipewir-pulse if they aren't running. To be ran with Sway

if pidof /usr/bin/pipewire; then
 echo PipeWire alredy running 
else
   /usr/bin/pipewire &
   echo PipeWire started
fi
if pidof /usr/bin/wireplumber; then
  echo WirePlumber already running 
else
   /usr/bin/wireplumber&
   echo WirePlumber started
fi
if pidof /usr/bin/pipewire-pulse; then
 echo PipeWire-Pulse already running 
else
   /usr/bin/pipewire-pulse &
   echo PipeWire-Pulse started
fi


